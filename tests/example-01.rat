fn X (x: Int) -> Int {
  return x + 1;
}

fn Z (mut x: Int) -> Int {
  x++;
  return x;
}

fn Y (f: fn (Int) -> Int) -> Int {
  return f(1);
}

fn A (f: fn (mut Int) -> Int) -> Int {
  let mut x = 1;
  return f(x);
}


fn MK (x: Int) -> fn (Int) -> Int {
  let mut a = x;
  fn INC(y: Int) -> Int {
    a = a + y;
    return a;
  }
  return INC;
}

fn G () -> fn (Int) -> Int {
  return X;
}

fn XDD() -> Int {
  if (1 > 0) {
    return 100;
  } else {
    return 20;
  }

  return 0;
}

